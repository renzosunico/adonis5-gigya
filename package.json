{
  "name": "adonis5-gigya",
  "version": "1.3.0",
  "description": "Gigya provider for AdonisJS 5",
  "author": "John Renzo Sunico",
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "lint": "eslint . --ext=.ts",
    "pretest": "npm run lint",
    "test": "node japa.js",
    "clean": "del-cli build",
    "compile": "npm run lint && npm run clean && tsc && npm run copy-files",
    "build": "npm run compile",
    "prepublishOnly": "npm run build",
    "release": "np --message=\"release-%s\"",
    "version": "npm run build",
    "format": "prettier --write .",
    "copy-files": "cp -R templates build/templates"
  },
  "devDependencies": {
    "@adonisjs/core": "^5.0.4-preview-rc-2.1",
    "@adonisjs/ioc-transformer": "^2.3.4",
    "@adonisjs/mrm-preset": "^5.0.3",
    "@adonisjs/require-ts": "^2.0.11",
    "@types/node": "^17.0.29",
    "adonis-provider-tester": "^1.3.0",
    "chai": "^4.3.6",
    "del-cli": "^4.0.1",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-adonis": "^2.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "japa": "^4.0.0",
    "mrm": "^4.0.0",
    "np": "^7.6.1",
    "pino-pretty": "^8.0.0",
    "prettier": "^2.6.2",
    "ts-node": "^10.8.1",
    "typescript": "^4.6.3",
    "yargs": "^17.5.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/renzosunico/adonis5-gigya.git"
  },
  "homepage": "https://github.com/renzosunico/adonis5-gigya#readme",
  "adonisjs": {
    "types": "adonis5-gigya",
    "providers": [
      "adonis5-gigya"
    ],
    "templates": {
      "basePath": "./build/templates",
      "config": [
        {
          "src": "config.txt",
          "dest": "gigya"
        }
      ]
    }
  },
  "license": "MIT",
  "main": "build/index.js",
  "files": [
    "build/adonis-typings",
    "build/src/providers",
    "build/templates",
    "build/index.js",
    "build/index.d.ts"
  ],
  "dependencies": {
    "gigya": "npm:@cambridge-pte/gigya@^3.0.2"
  },
  "mrmConfig": {
    "core": false,
    "license": "MIT",
    "services": [],
    "minNodeVersion": "16.13.1",
    "probotApps": [
      "stale",
      "lock"
    ]
  },
  "np": {
    "contents": ".",
    "anyBranch": false
  },
  "typings": "./build/adonis-typings/index.d.ts",
  "types": "./build/adonis-typings/index.d.ts",
  "keywords": [
    "Adonis",
    "5",
    "adonis-gigya"
  ]
}
